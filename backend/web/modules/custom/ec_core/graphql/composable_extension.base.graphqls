# type Mutation

scalar Violation
scalar Date
scalar Array

type User {
  id: Int!
  name: String!
  fullName: String
  phone: String
  email: String
  contact: Boolean
  categories: Array
  experiences: Array
  note: String
}

type Event {
  id: Int!
  title: String!
  body: String
  startDate: Date
  endDate: Date
  categories: Array
  volunteers: Array
}

type EventConnection {
  total: Int!
  items: [Event!]
}

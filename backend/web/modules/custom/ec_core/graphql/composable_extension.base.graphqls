# type Mutation
scalar Violation
scalar Date
scalar Array

type User {
  id: Int!
  name: String!
  email: String
  fullName: String
  phone: String
  primary: String
  contact: Boolean
  categories: Array
  experiences: Array
  note: String
  verified: Boolean
}

type Event {
  id: Int!
  title: String!
  body: String
  start: Date
  end: Date
  categories: Array
  volunteers: Array
}

type EventConnection {
  total: Int!
  items: [Event!]
}

type Taxonomy {
  id: Int!
  name: String
}

type TaxonomyConnection {
  total: Int!
  items: [Taxonomy!]
}
